<script>
    function copyCode() {
        // Select the text
        const code = event.target.previousElementSibling;
        const range = document.createRange();
        range.selectNodeContents(code);
        window.getSelection().removeAllRanges();
        window.getSelection().addRange(range);

        // Copy the selected text
        document.execCommand('copy');

        // // Alert the user
        // alert('Code copied to clipboard!');
    }

    // Include header and footer
    // document.getElementById('header').innerHTML = fetch('../../../pages/tips/tips_breadcrumb.html').then(res => res.text());

    async function loadHeader() {
        const response = await fetch('../../../pages/tips/tips_breadcrumb.html');
        const htmlContent = await response.text();
        document.getElementById('header').innerHTML = htmlContent;
    }

    loadHeader();
</script>